{
  "name": "Redux-Music",
  "version": "1.0.0",
  "description": "A Simple Client for SoundCloud",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.prod.config.js",
    "lint": "eslint scripts",
    "start": "webpack-dev-server --progress --colors --hot --config ./webpack.config.js",
    "test": "mocha --compilers js:babel-core/register --recursive -r mock-local-storage",
    "prebuild": "sed -i.bak \"s/'.*'/'${SOUND_REDUX_PROD:-f4323c6f7c0cd73d2d786a2b1cdae80c}'/\" ./scripts/constants/Config.js",
    "postbuild": "sed -i.bak \"s/'.*'/'${SOUND_REDUX_DEV:-f4323c6f7c0cd73d2d786a2b1cdae80c}'/\" ./scripts/constants/Config.js && rm ./scripts/constants/Config.js.bak"
  },
  "author": "AngryBugs",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze-strict": "^1.1.1",
    "eslint-config-rallycoding": "^3.1.0",
    "eslint-plugin-react": "^6.2.2",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^3.2.1",
    "foundation-sites": "^6.2.3",
    "immutable": "^3.8.1",
    "jquery": "^2.2.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "moment": "^2.14.1",
    "node-env-file": "^0.1.8",
    "node-sass": "^3.8.0",
    "node-uuid": "^1.4.7",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-test-utils": "^15.3.0",
    "react-animate": "^3.0.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
