{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es7"],
    "baseUrl": "./",
    "paths": {
      "@soundnode-redux/client/src/*": ["src/*"]
    }
  },
  "include": ["./src/**/*"]
}
